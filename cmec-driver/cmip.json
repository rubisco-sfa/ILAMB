            "weight": 9.0,
            "skip_iav": true
          },
          "DOLCE": {
            "source": "DATA/et/DOLCE/DOLCE.nc",
            "weight": 15.0,
            "land": true
          }
        },
        "Runoff": {
          "variable": "runoff",
          "alternate_vars": "mrro",
          "weight": 5.0,
          "mass_weighting": true,
          "Dai": {
            "ctype": "ConfRunoff",
