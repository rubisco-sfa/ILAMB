          },
          "GEWEX.SRB": {
            "source": "DATA/albedo/GEWEX.SRB/albedo_0.5x0.5.nc",
            "weight": 20.0
          }
        },
        "Surface Upward SW Radiation": {
          "variable": "rsus",
